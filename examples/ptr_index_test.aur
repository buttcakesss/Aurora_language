fn main() -> i64 {
  // Allocate space for 5 i64s
  let p: ptr<i64> = malloc(40);  // 5 * 8 bytes
  
  // Write values using pointer indexing
  p[0] = 10;
  p[1] = 20;
  p[2] = 30;
  p[3] = 40;
  p[4] = 50;
  
  // Read and sum using pointer indexing
  let sum: i64 = p[0] + p[1] + p[2] + p[3] + p[4];
  
  free(p);
  
  return sum;  // Should return 150
}